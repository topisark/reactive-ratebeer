{
  "extends": "airbnb",
  "rules": {
    "eol-last": "off",
    "react/prefer-stateless-function": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "document": true
  }
}